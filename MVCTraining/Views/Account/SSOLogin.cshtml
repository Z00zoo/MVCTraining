@model MVCTraining.Models.SSOViewModel

@using (Html.BeginForm("Login", "Account", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.Verifykey)
}

@section scripts
{
    <script>
        var vkey = "@Model.Verifykey";

        if (vkey != "") {
            $("form").submit();
        }

    </script>
}


